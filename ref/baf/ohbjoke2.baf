    HaveSpell(BLADE_OFFENSIVE_SPIN)  // SPCL521.SPL (Offensive Spin)
    CheckStat(Myself,0,IMPROVEDHASTE)
    !CheckStat(Myself,2,OFFENSIVE_MODIFIER)
    Range(NearestEnemyOf(Myself),6)
    !GlobalTimerNotExpired("OH_Cast","LOCALS")
    !GlobalTimerNotExpired("OH_OSpin","LOCALS")
THEN
    RESPONSE #100
        SetGlobalTimer("OH_Cast","LOCALS",ONE_ROUND)
        SetGlobalTimer("OH_OSpin","LOCALS",FOUR_ROUNDS)
        Spell(Myself,BLADE_OFFENSIVE_SPIN)  // SPCL521.SPL (Offensive Spin)
END

